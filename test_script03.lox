class Confection {
    issweet() {
        return true;
    }
}

class Cake < Confection {
    init(flavor) {
        this.flavor = flavor;
    }

    taste() {
        var adjective = "delicious";
        print "The " + this.flavor + " cake is " + adjective + "!";
    }
}

var cake = Cake("German chocolate");
cake.taste();